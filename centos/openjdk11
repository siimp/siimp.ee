curl -O https://download.java.net/java/GA/jdk11/9/GPL/openjdk-11.0.2_linux-x64_bin.tar.gz
tar zxvf openjdk-11.0.2_linux-x64_bin.tar.gz
mv jdk-11.0.2 /usr/local/jvm/jdk-11.0.2

# alternatives --remove /usr/bin/java java /usr/local/jdk-10/bin/java

alternatives --install /usr/bin/java java /usr/local/jvm/jdk-11.0.2/bin/java 2
alternatives --config java
