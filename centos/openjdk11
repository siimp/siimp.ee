curl -O https://download.java.net/openjdk/jdk11/ri/openjdk-11+28_linux-x64_bin.tar.gz
tar zxvf openjdk-11+28_linux-x64_bin.tar.gz
mv jdk-11 /usr/local/jvm/jdk-11

# alternatives --remove /usr/bin/java java /usr/local/jdk-10/bin/java

alternatives --install /usr/bin/java java /usr/local/jvm/jdk-11/bin/java 1
alternatives --config java
